  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:local": "env.bat && yarn run prebuild && nest start --watch",
    "start:dev": "yarn run prebuild && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm:migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "typeorm:migration:generate": "yarn run typeorm migration:generate",
    "typeorm:reset": "yarn run build && yarn run typeorm schema:drop && yarn run typeorm:migration:run"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.6.0",
    "@nestjs/common": "^7.5.1",
    "@nestjs/core": "^7.5.1",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.6.14",
    "@nestjs/platform-fastify": "^7.5.1",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/swagger": "4.6.1",
    "@nestjs/typeorm": "^7.1.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/prettier": "^2.2.3",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "concat-stream": "^2.0.0",
    "fastify-helmet": "^4.0.2",
    "fastify-multer": "^2.0.2",
    "fastify-multipart": "^2.0.3",
    "fastify-swagger": "3.3.0",
    "file-type": "^14.6.2",
    "handlebars": "^4.7.6",
    "hcaptcha": "^0.0.2",
    "luxon": "^1.25.0",
    "nestjs-i18n": "^8.0.10",
    "passport": "^0.4.1",
    "passport-headerapikey": "^1.2.1",
    "passport-jwt": "^4.0.0",
    "pdfmake": "^0.1.69",
    "pg": "^8.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "soundex-code": "^1.0.5",
    "typeorm": "^0.2.32"
